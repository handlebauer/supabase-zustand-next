{
    "name": "supabase-zustand-next",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky",
        "format": "prettier --write .",
        "supabase:gen-types": "bunx supabase gen types typescript --local > src/lib/supabase/types.ts",
        "supabase:init": "bunx supabase db reset",
        "supabase:seed:dev": "bun run scripts/db/dev.ts",
        "supabase:seed:test": "bun run scripts/db/test.ts",
        "shadcn:add": "bunx --bun shadcn@latest add",
        "postinstall": "bun run .template/install.ts",
        "test": "playwright test --config=playwright.config.ts"
    },
    "dependencies": {
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-slot": "^1.1.1",
        "@supabase/ssr": "^0.5.2",
        "@tabler/icons-react": "^3.28.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "lucide-react": "^0.473.0",
        "next": "15.1.5",
        "next-themes": "^0.4.4",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.1",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@playwright/test": "^1.49.1",
        "@types/bun": "^1.1.17",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "dotenv": "^16.4.7",
        "eslint": "^9",
        "eslint-config-next": "15.1.5",
        "eslint-config-prettier": "^10.0.1",
        "husky": "^9.1.7",
        "playwright": "^1.49.1",
        "postcss": "^8",
        "prettier": "3.4.2",
        "prompt-sync": "^4.2.0",
        "tailwindcss": "^3.4.1",
        "typescript": "^5"
    }
}
